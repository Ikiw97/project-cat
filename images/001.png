<svg width="300" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="catGradient1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4ecdc4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff6b6b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Cat body -->
  <ellipse cx="150" cy="180" rx="70" ry="50" fill="url(#catGradient1)" />
  
  <!-- Cat head -->
  <circle cx="150" cy="110" r="40" fill="url(#catGradient1)" />
  
  <!-- Cat ears -->
  <polygon points="125,80 135,95 115,95" fill="url(#catGradient1)" />
  <polygon points="175,80 185,95 165,95" fill="url(#catGradient1)" />
  
  <!-- Cat eyes -->
  <circle cx="135" cy="105" r="7" fill="#fff" />
  <circle cx="165" cy="105" r="7" fill="#fff" />
  <circle cx="135" cy="105" r="3" fill="#000" />
  <circle cx="165" cy="105" r="3" fill="#000" />
  
  <!-- Cat nose -->
  <polygon points="150,115 145,125 155,125" fill="#ff69b4" />
  
  <!-- Cat mouth -->
  <path d="M 150 125 Q 140 135 135 130" stroke="#000" stroke-width="2" fill="none" />
  <path d="M 150 125 Q 160 135 165 130" stroke="#000" stroke-width="2" fill="none" />
  
  <!-- Cat whiskers -->
  <line x1="110" y1="110" x2="130" y2="115" stroke="#000" stroke-width="2" />
  <line x1="110" y1="120" x2="130" y2="120" stroke="#000" stroke-width="2" />
  <line x1="170" y1="115" x2="190" y2="110" stroke="#000" stroke-width="2" />
  <line x1="170" y1="120" x2="190" y2="120" stroke="#000" stroke-width="2" />
  
  <!-- Cat paws -->
  <circle cx="120" cy="230" r="15" fill="url(#catGradient1)" />
  <circle cx="180" cy="230" r="15" fill="url(#catGradient1)" />
  
  <!-- Cat tail -->
  <path d="M 220 180 Q 240 150 220 120" stroke="url(#catGradient1)" stroke-width="15" fill="none" />
</svg>
