<svg width="300" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="catGradient5" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fd79a8;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e84393;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="moonGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#dfe6e9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#b2bec3;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Moon -->
  <circle cx="150" cy="150" r="100" fill="url(#moonGradient)" />
  <circle cx="120" cy="120" r="20" fill="#b2bec3" opacity="0.5" />
  <circle cx="180" cy="100" r="15" fill="#b2bec3" opacity="0.5" />
  <circle cx="190" cy="170" r="25" fill="#b2bec3" opacity="0.5" />
  
  <!-- Cat on moon -->
  <ellipse cx="150" cy="180" rx="40" ry="30" fill="url(#catGradient5)" />
  
  <!-- Cat head -->
  <circle cx="150" cy="130" r="30" fill="url(#catGradient5)" />
  
  <!-- Cat ears -->
  <polygon points="130,110 140,120 120,120" fill="url(#catGradient5)" />
  <polygon points="170,110 180,120 160,120" fill="url(#catGradient5)" />
  
  <!-- Cat eyes (starry) -->
  <circle cx="140" cy="125" r="5" fill="#fff" />
  <circle cx="160" cy="125" r="5" fill="#fff" />
  <polygon points="140,125 142,128 145,125 142,122" fill="#ffd32a" />
  <polygon points="160,125 162,128 165,125 162,122" fill="#ffd32a" />
  
  <!-- Cat nose -->
  <polygon points="150,135 145,145 155,145" fill="#ff69b4" />
  
  <!-- Cat mouth (amazed) -->
  <circle cx="150" cy="155" r="5" fill="#000" />
  
  <!-- Cat whiskers -->
  <line x1="115" y1="130" x2="135" y2="135" stroke="#000" stroke-width="2" />
  <line x1="115" y1="140" x2="135" y2="140" stroke="#000" stroke-width="2" />
  <line x1="165" y1="135" x2="185" y2="130" stroke="#000" stroke-width="2" />
  <line x1="165" y1="140" x2="185" y2="140" stroke="#000" stroke-width="2" />
  
  <!-- Cat paws -->
  <circle cx="135" cy="200" r="8" fill="url(#catGradient5)" />
  <circle cx="165" cy="200" r="8" fill="url(#catGradient5)" />
  
  <!-- Cat tail (excited) -->
  <path d="M 190 180 Q 210 170 200 150 Q 190 130 170 140" stroke="url(#catGradient5)" stroke-width="8" fill="none" />
  
  <!-- Stars -->
  <polygon points="50,50 52,56 58,56 53,60 55,66 50,62 45,66 47,60 42,56 48,56" fill="#ffd32a" />
  <polygon points="250,70 252,76 258,76 253,80 255,86 250,82 245,86 247,80 242,76 248,76" fill="#ffd32a" />
  <polygon points="70,200 72,206 78,206 73,210 75,216 70,212 65,216 67,210 62,206 68,206" fill="#ffd32a" />
  <polygon points="230,220 232,226 238,226 233,230 235,236 230,232 225,236 227,230 222,226 228,226" fill="#ffd32a" />
</svg>
