<svg width="300" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="catGradient3" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#6c5ce7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#a29bfe;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Cat jumping -->
  <ellipse cx="150" cy="150" rx="50" ry="40" fill="url(#catGradient3)" transform="rotate(-15 150 150)" />
  
  <!-- Cat head -->
  <circle cx="120" cy="120" r="35" fill="url(#catGradient3)" />
  
  <!-- Cat ears -->
  <polygon points="100,95 110,110 90,110" fill="url(#catGradient3)" />
  <polygon points="140,95 150,110 130,110" fill="url(#catGradient3)" />
  
  <!-- Cat eyes (excited) -->
  <circle cx="110" cy="115" r="6" fill="#fff" />
  <circle cx="130" cy="115" r="6" fill="#fff" />
  <circle cx="110" cy="115" r="3" fill="#000" />
  <circle cx="130" cy="115" r="3" fill="#000" />
  
  <!-- Cat nose -->
  <polygon points="120,125 115,135 125,135" fill="#ff69b4" />
  
  <!-- Cat mouth (excited) -->
  <ellipse cx="120" cy="140" rx="8" ry="5" fill="#000" />
  
  <!-- Cat whiskers -->
  <line x1="85" y1="120" x2="105" y2="125" stroke="#000" stroke-width="2" />
  <line x1="85" y1="130" x2="105" y2="130" stroke="#000" stroke-width="2" />
  <line x1="135" y1="125" x2="155" y2="120" stroke="#000" stroke-width="2" />
  <line x1="135" y1="130" x2="155" y2="130" stroke="#000" stroke-width="2" />
  
  <!-- Cat paws (in motion) -->
  <circle cx="110" cy="180" r="10" fill="url(#catGradient3)" />
  <circle cx="140" cy="190" r="10" fill="url(#catGradient3)" />
  <circle cx="160" cy="170" r="10" fill="url(#catGradient3)" />
  <circle cx="190" cy="180" r="10" fill="url(#catGradient3)" />
  
  <!-- Cat tail (in motion) -->
  <path d="M 200 150 Q 220 130 210 110 Q 200 90 180 100" stroke="url(#catGradient3)" stroke-width="10" fill="none" />
  
  <!-- Motion lines -->
  <line x1="50" y1="200" x2="80" y2="195" stroke="#fff" stroke-width="2" opacity="0.7" />
  <line x1="60" y1="210" x2="90" y2="205" stroke="#fff" stroke-width="2" opacity="0.7" />
  <line x1="70" y1="220" x2="100" y2="215" stroke="#fff" stroke-width="2" opacity="0.7" />
</svg>
