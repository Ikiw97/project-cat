<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="catGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff6b6b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4ecdc4;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Cat body -->
  <ellipse cx="200" cy="200" rx="80" ry="60" fill="url(#catGradient)" />
  
  <!-- Cat head -->
  <circle cx="200" cy="120" r="50" fill="url(#catGradient)" />
  
  <!-- Cat ears -->
  <polygon points="170,80 180,100 160,100" fill="url(#catGradient)" />
  <polygon points="230,80 240,100 220,100" fill="url(#catGradient)" />
  
  <!-- Cat eyes -->
  <circle cx="185" cy="115" r="8" fill="#fff" />
  <circle cx="215" cy="115" r="8" fill="#fff" />
  <circle cx="185" cy="115" r="4" fill="#000" />
  <circle cx="215" cy="115" r="4" fill="#000" />
  
  <!-- Cat nose -->
  <polygon points="200,125 195,135 205,135" fill="#ff69b4" />
  
  <!-- Cat mouth -->
  <path d="M 200 135 Q 190 145 185 140" stroke="#000" stroke-width="2" fill="none" />
  <path d="M 200 135 Q 210 145 215 140" stroke="#000" stroke-width="2" fill="none" />
  
  <!-- Cat whiskers -->
  <line x1="150" y1="120" x2="170" y2="125" stroke="#000" stroke-width="2" />
  <line x1="150" y1="130" x2="170" y2="130" stroke="#000" stroke-width="2" />
  <line x1="230" y1="125" x2="250" y2="120" stroke="#000" stroke-width="2" />
  <line x1="230" y1="130" x2="250" y2="130" stroke="#000" stroke-width="2" />
  
  <!-- Text -->
  <text x="200" y="280" text-anchor="middle" font-family="Comic Sans MS, cursive" font-size="24" fill="#fff">Coordination Cat</text>
</svg>
